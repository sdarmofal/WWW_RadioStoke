<table border="0" cellpadding="2" cellspacing="1" class="forumline" width="100%"> 
	<tr>
		<th class="thTop" align="center" colspan="2">{L_S_AGENT_TITLE}</td>
	</tr>
	<!-- BEGIN s_loop -->
	<tr>
		<td class="{s_loop.CLASS}" align="center" width="10%"><span class="gen"><img src="{s_loop.SYSTEM}" /></span></td>
		<td class="{s_loop.CLASS}" align="left" width="90%"><span class="nav">{s_loop.VALUE}<br />
			<table cellspacing="0" cellpadding="0" border="0" align="left">
				<tr>
					<td align="right"><img src="{LEFT_GRAPH_IMAGE}" alt="" width="4" height="12" /></td>
				</tr>
			</table>
			<table cellspacing="0" cellpadding="0" border="0" align="left" width="{s_loop.BAR}%">
				<tr>
					<td><img src="{GRAPH_IMAGE}" alt="" width="100%" height="12" /></td>
				</tr>
			</table>
			<table cellspacing="0" cellpadding="0" border="0" align="left">
				<tr>
					<td align="left"><img src="{RIGHT_GRAPH_IMAGE}" alt="" width="4" height="12" /></td>
				</tr>
			</table>
		</span></td>
	</tr>
	<!-- END s_loop -->
</table>
